{
  "name": "customer-api",
  "version": "1.0.0",
  "description": "Customer's API that provides end/entrypoints to fetch consumer's data",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "build:start": "npm run build && npm run start",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm run test -- -c jest-unit.config.js",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:integration": "npm run test -- -c jest-integration.config.js",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:e2e": "npm run test -- -c jest-e2e.config.js",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:ci": "npm test -- --coverage",
    "test:staged": "npm run test:unit -- --findRelatedTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://vinicius91carvalho@bitbucket.org/vinicius91carvalho/customer-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/vinicius91carvalho/customer-api/issues"
  },
  "homepage": "https://bitbucket.org/vinicius91carvalho/customer-api#readme",
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "eslint": "7.21.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "git-commit-msg-linter": "3.1.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "module-alias": "2.2.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
